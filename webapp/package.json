    "@types/react-router-dom": "5.1.5",
    "@types/react-transition-group": "4.4.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "css-loader": "^5.0.2",
